<% if session[:queue].count == 1 %>
  $("#tint").html("<%= escape_javascript(render partial: '/players/player_hud', locals: {song: @song}) %>");
  playSong(<%= @song.sc_id.to_json %>)
<% end %>
$("#next-window").html("<%= escape_javascript(render partial: '/players/queue', locals: {songqueue: @songqueue}) %>");
